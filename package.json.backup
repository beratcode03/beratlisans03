    "electron:build:test": "npm run electron:encode-config && npm run electron:prepare-package && vite build && npm run build-server-electron && npm run electron:preflight && electron-builder build --win --x64 --dir && npm run electron:restore-package",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:self-destruct": "node scripts/test-self-destruct.cjs",
    "set-destruct-date": "node scripts/set-destruct-date.cjs",
    "verify-destruct-date": "node scripts/verify-destruct-date.cjs",
    "set-license-key": "node scripts/set-license-key.cjs"
