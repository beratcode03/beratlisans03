    "start": "cross-env NODE_ENV=production node server/dist/index.js",
    "electron": "electron electron/main.cjs",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "npm run electron:encode-config && npm run electron:prepare-package && vite build && npm run build-server-electron && npm run protect-all && npm run electron:preflight && electron-builder build --win --x64 && npm run electron:restore-package",
    "electron:build:full": "npm run electron:encode-config && npm run electron:prepare-package && npm run prepare-build-config && vite build && npm run build-server-electron && npm run protect-all && npm run electron:preflight && electron-builder build --win --x64 && npm run electron:restore-package",
